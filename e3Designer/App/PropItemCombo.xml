<Element
  width="90%"
  height="30dp">
  <include path="UiColor.h"/>
  <Text
    element="mTitle"
    fontSize="12dp"
    textAlignHor="Start"
    width="50%">
  </Text>
  <Element
    width="1dp"
    margin="2dp"
    height="50%"
    backgroundColor="glm::vec4(180, 180, 180, 255)">
  </Element>
  <UiComboBox
    element="mCombo"
    width="50%"
    height="100%">
    <UiComboBoxHeaderDefault
        element="mComboHeader">
    </UiComboBoxHeaderDefault>
    <UiComboBoxBody
        element="mComboBody">
    </UiComboBoxBody>
  </UiComboBox>
</Element>